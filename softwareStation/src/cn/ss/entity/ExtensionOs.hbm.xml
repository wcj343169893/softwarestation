<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.ss.entity.ExtensionOs" table="extensionOs" catalog="softwarestation">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="phoneOs" type="java.lang.Integer">
			<column name="phoneOs" length="20">
				<comment>平台</comment>
			</column>
		</property>
		<property name="extension" type="java.lang.Integer">
			<column name="extension" length="20">
				<comment>支持扩展名</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
